{
  "name": "example-create-react-app-express",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "start:dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js ",
    "install": "cd client && npm install && npm run build && cd .."
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.2",
    "http-proxy-middleware": "^0.18.0",
    "jquery": "^3.3.1",
    "nodemailer": "^4.6.7",
    "react-image-gallery": "^0.8.11",
    "react-player": "^1.6.6"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  }
}
